<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
	<ACTION NAME="addFile">
		<CODE>
			org.liujing.jeditplugin.MyView.getInstance(view).popupAddFileDialog(view.getBuffer().getPath());
		</CODE>
	</ACTION>
	<ACTION NAME="OSXfull">
	    <CODE>
	    com.apple.eawt.FullScreenUtilities.setWindowCanFullScreen(view, true);
	    </CODE>
	</ACTION>
	<ACTION NAME="togOSXfull">
	<CODE>
	    org.liujing.jeditplugin.MyJeditPlugin.toggleOSXFullscreen(view);
	    </CODE>
	</ACTION>
	<ACTION NAME="openAction">
		<CODE>
			new org.liujing.jeditplugin.MyJeditPluginPanel();
		</CODE>
	</ACTION>
	<ACTION NAME="getClassName">
		<CODE>
			org.liujing.jeditplugin.v2.JavaFileAnalysisJEditDelegate.getInstance().copyFullClassName();
		</CODE>
	</ACTION>
	<ACTION NAME="checkImports">
		<CODE>
			org.liujing.jeditplugin.v2.JavaFileAnalysisJEditDelegate.getInstance().checkImports();
		</CODE>
	</ACTION>
	<ACTION NAME="copyAssociatedTypes">
		<CODE>
			org.liujing.jeditplugin.v2.JavaFileAnalysisJEditDelegate.getInstance().copyAllAssociatedTypes();
		</CODE>
	</ACTION>
	<ACTION NAME="copyClassTemplateA">
